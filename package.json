  "bugs": {
    "url": "https://github.com/bakhtw1/AI_TEXT_EDITOR_4250/issues"
  },
  "homepage": "https://github.com/bakhtw1/AI_TEXT_EDITOR_4250#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
