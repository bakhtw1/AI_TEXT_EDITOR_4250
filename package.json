    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/webpack-dev-server": "^4.7.2",
    "@uiw/react-monacoeditor": "^3.5.2",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "electron": "^17.0.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
